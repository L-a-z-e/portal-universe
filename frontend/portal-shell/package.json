{
  "name": "@portal/portal-shell",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite build --watch --mode dev\" \"vite preview --port 30000 --strictPort --mode dev\"",
    "build": "vue-tsc -b && vite build --mode dev",
    "build:dev": "vue-tsc -b && vite build --mode dev",
    "build:docker": "vue-tsc -b && vite build --mode docker",
    "build:k8s": "vue-tsc -b && vite build --mode k8s",
    "preview": "vite preview --port 30000 --strictPort",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@portal/design-core": "*",
    "@portal/design-vue": "*",
    "@stomp/stompjs": "^7.3.0",
    "axios": "^1.12.2",
    "pinia": "^3.0.3",
    "sockjs-client": "^1.6.1",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@types/node": "^24.8.1",
    "@types/sockjs-client": "^1.5.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.49",
    "sass-embedded": "^1.93.3",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.0.7"
  }
}
