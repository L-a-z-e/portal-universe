{
  "name": "@portal/blog-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite build --watch --mode dev\" \"vite preview --port 30001 --strictPort --mode dev\"",
    "build": "vue-tsc -b && vite build --mode dev",
    "build:dev": "vue-tsc -b && vite build --mode dev",
    "build:docker": "vue-tsc -b && vite build --mode docker",
    "build:k8s": "vue-tsc -b && vite build --mode k8s",
    "preview": "vite preview --port 30001 --strictPort",
    "clean": "rm -rf dist",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@portal/design-core": "*",
    "@portal/design-vue": "*",
    "@portal/vue-bridge": "*",
    "@toast-ui/editor": "^3.2.2",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.1.0",
    "axios": "^1.12.2",
    "concurrently": "^9.2.1",
    "oidc-client-ts": "^3.3.0",
    "pinia": "^3.0.3",
    "prismjs": "^1.30.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^22.19.7",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.0.7"
  }
}
