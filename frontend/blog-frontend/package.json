{
  "name": "blog-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite build --watch --mode dev\" \"vite preview --port 30001 --strictPort --mode dev\"",
    "build:dev": "vue-tsc -b && vite build --mode dev",
    "build:docker": "vue-tsc -b && vite build --mode docker",
    "build:k8s": "vue-tsc -b && vite build --mode k8s",
    "preview": "vite preview --port 30001 --strictPort"
  },
  "dependencies": {
    "@types/node": "^24.8.1",
    "axios": "^1.12.2",
    "concurrently": "^9.2.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.0.7"
  }
}
