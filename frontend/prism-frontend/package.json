{
  "name": "@portal/prism-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "AI Agent Orchestration Kanban Board for Portal Universe",
  "scripts": {
    "dev": "concurrently \"vite build --watch --mode dev\" \"vite preview --port 30003 --strictPort --mode dev\"",
    "build": "tsc --noEmit && vite build",
    "build:dev": "tsc -b && vite build --mode dev",
    "build:docker": "tsc -b && vite build --mode docker",
    "build:k8s": "tsc -b && vite build --mode k8s",
    "preview": "vite preview --port 30003 --strictPort",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@portal/design-core": "*",
    "@portal/design-react": "*",
    "@portal/react-bootstrap": "*",
    "@portal/react-bridge": "*",
    "axios": "^1.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^7.2.0"
  }
}
